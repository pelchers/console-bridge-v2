{
  "name": "console-bridge",
  "version": "0.1.0",
  "description": "Stream browser console output from localhost applications to your terminal",
  "main": "src/server/index.js",
  "bin": {
    "console-bridge": "./bin/console-bridge.js"
  },
  "scripts": {
    "start": "node bin/console-bridge.js start",
    "dev": "nodemon bin/console-bridge.js start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "format": "prettier --write \"src/**/*.js\"",
    "prepare": "npm run build:client",
    "build:client": "node scripts/build-client.js"
  },
  "keywords": [
    "console",
    "terminal",
    "logging",
    "debug",
    "developer-tools",
    "localhost",
    "websocket",
    "bridge",
    "stream",
    "realtime"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/console-bridge.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/console-bridge/issues"
  },
  "homepage": "https://github.com/yourusername/console-bridge#readme",
  "files": [
    "bin/",
    "src/",
    "README.md",
    "LICENSE"
  ]
}
