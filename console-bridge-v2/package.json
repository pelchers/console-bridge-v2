{
  "name": "console-bridge",
  "version": "1.0.0",
  "description": "Bridge browser console logs from localhost applications to your terminal with multi-instance support",
  "main": "src/index.js",
  "bin": {
    "console-bridge": "./bin/console-bridge.js"
  },
  "scripts": {
    "start": "node bin/console-bridge.js",
    "dev": "nodemon bin/console-bridge.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.js bin/**/*.js",
    "lint:fix": "eslint src/**/*.js bin/**/*.js --fix",
    "format": "prettier --write \"src/**/*.js\" \"bin/**/*.js\" \"test/**/*.js\"",
    "format:check": "prettier --check \"src/**/*.js\" \"bin/**/*.js\" \"test/**/*.js\"",
    "prepublishOnly": "npm test && npm run lint"
  },
  "keywords": [
    "console",
    "browser",
    "devtools",
    "logging",
    "terminal",
    "localhost",
    "debugging",
    "puppeteer",
    "chrome",
    "devtools",
    "cli",
    "monitor",
    "log-streaming"
  ],
  "author": "Your Name",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "bin/",
    "src/",
    "docs/",
    "examples/",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "puppeteer": "^21.0.0",
    "strip-ansi": "^6.0.1"
  },
  "devDependencies": {
    "eslint": "^8.50.0",
    "jest": "^29.0.0",
    "nodemon": "^3.0.0",
    "prettier": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/console-bridge.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/console-bridge/issues"
  },
  "homepage": "https://github.com/yourusername/console-bridge#readme"
}
