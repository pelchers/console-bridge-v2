{
  "manifest_version": 3,
  "name": "Console Bridge",
  "version": "2.0.0",
  "description": "Bridge browser console logs from localhost applications to your terminal in real-time",
  "permissions": ["devtools"],
  "host_permissions": ["http://localhost/*", "http://127.0.0.1/*"],
  "devtools_page": "devtools/devtools.html",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
